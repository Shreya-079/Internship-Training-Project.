    "import string\n",
    "import secrets\n",
    "chars=string.ascii_letters + string.digits + string.punctuation\n",
    "length=int(input(\"Enter Your Password length: \"))\n",
    "password = \"\".join(secrets.choice(chars)\n",
    "                    for x in range(length))\n",
    "print(password)"
